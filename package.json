{
  "name": "zen-flow",
  "version": "2.2.0",
  "description": "MineTweaker ZenScript made easy.",
  "main": "dist/zenflow.cjs",
  "module": "dist/zenflow.mjs",
  "types": "dist/zenflow.d.ts",
  "exports": {
    ".": {
      "import": "./dist/zenflow.mjs",
      "require": "./dist/zenflow.cjs"
    }
  },
  "keywords": [
    "minetweaker",
    "minecraft",
    "zenscript"
  ],
  "files": [
    "dist/**/*"
  ],
  "repository": "https://github.com/chronoDave/zen-flow.git",
  "author": "Chronocide",
  "license": "MIT",
  "engines": {
    "node": ">10.12.0"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "rollup -c",
    "test": "tsc && tape test/**/*.spec.js",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "esbuild": "^0.14.25",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "rollup": "^2.70.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "tape": "^5.5.2",
    "typescript": "^4.6.2"
  }
}
